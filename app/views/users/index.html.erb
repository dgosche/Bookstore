<header id="content-header">
	<h1><%= pluralize(@users.size, 'User') %></h1>
</header>

<ul id="users">
	<% @users.each do |user| %>
		<li>
			<%= link_to user.name, user %>
		</li>
	<% end %>
</ul>

<footer id="content-footer">
	<nav>
		<p>
			<%= link_to "Create User", new_user_path, class: "button" %>
		</p>
	</nav>
</footer>